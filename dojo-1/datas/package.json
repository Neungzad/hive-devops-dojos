{
  "name": "dojo-1",
  "version": "1.0.0",
  "description": "Web and Mobile testing with Appium",
  "main": "index.js",
  "author": "Palo IT",
  "license": "MIT",
  "vars": {
    "web": "cucumber-js features/**/*.feature -f node_modules/cucumber-pretty --require-module @babel/register --require-module @babel/polyfill -f @cucumber/pretty-formatter -f json:report/cucumber_report.json  -r steps/web",
    "android": "cucumber-js features/**/*.feature -f node_modules/cucumber-pretty --require-module @babel/register --require-module @babel/polyfill -f @cucumber/pretty-formatter -f json:report-android/cucumber_report.json  -r steps/mobile",
    "ios": "cucumber-js features/**/*.feature -f node_modules/cucumber-pretty --require-module @babel/register --require-module @babel/polyfill -f @cucumber/pretty-formatter -f json:report-ios/cucumber_report.json  -r steps/mobile"
  },
  "scripts": {
    "appium": "$(npm bin)/appium --port 4444",
    "test": "NODE_ENV=PLATFORM=web $npm_package_vars_web --retry 1 --tags '(not @skip and not @mobile)'",
    "focus": "NODE_ENV=PLATFORM=web $npm_package_vars_web --tags '(@focus and not @mobile)'",
    "test-parallel": "NODE_OPTIONS='--unhandled-rejections=warn --trace-warnings' NODE_ENV=PLATFORM=web $npm_package_vars_web --retry 1 --tags '(not @mobile and not @noparallel)' --parallel 3",
    "test-non-parallel": "NODE_OPTIONS='--unhandled-rejections=warn --trace-warnings' NODE_ENV=PLATFORM=web $npm_package_vars_web --retry 1 --tags '(@noparallel and not @mobile)'",
    "android-remote-v3": "NODE_ENV=PLATFORM=android TEST_ENV=remote $npm_package_vars_android --retry 2 --tags '(@android and not @skip)'",
    "android-remote-v3-focus": "NODE_ENV=PLATFORM=android TEST_ENV=remote $npm_package_vars_android --retry 2 --tags '(@android and @focus and not @skip)'",
    "android-local-v3": "NODE_ENV=PLATFORM=android TEST_ENV=local $npm_package_vars_android --tags '(@android and not @skip)'",
    "android-local-v3-focus": "NODE_ENV=PLATFORM=android TEST_ENV=local $npm_package_vars_android --tags '(@android and @focus and not @skip)'",
    "ios-remote-v3": "NODE_ENV=PLATFORM=ios TEST_ENV=remote $npm_package_vars_ios --retry 2 --tags '(@ios and not @skip)'",
    "ios-remote-v3-focus": "NODE_ENV=PLATFORM=ios TEST_ENV=remote $npm_package_vars_ios --retry 2 --tags '(@ios and @focus and not @skip)'",
    "ios-local-v3": "NODE_ENV=PLATFORM=ios TEST_ENV=local $npm_package_vars_ios --tags '(@ios and not @skip)'",
    "ios-local-v3-focus": "NODE_ENV=PLATFORM=ios TEST_ENV=local $npm_package_vars_ios --tags '(@ios and @focus and not @skip)'"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@cucumber/cucumber": "^7.0.0",
    "@cucumber/pretty-formatter": "^1.0.0-alpha.0",
    "app-root-path": "^3.0.0",
    "appium": "^1.20.2",
    "axios": "^0.21.1",
    "chromedriver": "^91.0",
    "chromedriver-webview": "npm:chromedriver@^74.0.0",
    "cucumber-json-merge": "0.0.7",
    "dayjs": "^1.9.6",
    "dotenv": "^8.2.0",
    "form-data": "^3.0.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "mocha": "^8.2.1",
    "node-resemble-js": "^0.2.0",
    "resemblejs": "^3.2.5",
    "selenium-webdriver": "^4.0.0-beta.4",
    "sharp": "^0.28.1",
    "wd": "^1.14.0",
    "webdriverio": "^7.7.4"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "cucumber-html-reporter": "^5.3.0",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "multiple-cucumber-html-reporter": "^1.18.0"
  }
}
